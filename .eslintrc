{
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "__DEBUG_NEW_WINDOW__": false,
    "React": true,
    "ReactDOM": true
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "react-in-jsx-scope": 0,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "no-trailing-spaces": 0
  }
}