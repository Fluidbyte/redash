    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon-chai": "^1.1.0",
    "karma-spec-reporter": "0.0.22",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "rollup": "^0.21.0",
